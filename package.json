{
  "name": "kingdoms",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "orval": "orval",
    "prepare": "husky install",
    "preupdate": "npx npm-check-updates -u",
    "update": "npm run update",
    "postupdate": "npm install"
  },
  "lint-staged": {
    "*.css": [
      "stylelint --fix --cache"
